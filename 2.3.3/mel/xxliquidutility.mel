
proc log2(string $prefix, string $log)
{
	print($prefix +">"+ $log +"\n");
}

global proc liqlog(string $log)
{
	log2("liquid", $log);
}

global proc liqDebug(string $log)
{
	log2("DEBUG", $log);
}
global proc liqError(string $log)
{
	log2("ERROR", $log);
}
global proc liqWarn(string $log)
{
	log2("WARNNING", $log);
}
global proc liqTrace(string $log)
{
	trace($log);
}